{
    "version": 2,
    "builds": [
        {
            "src": "./server/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes" : [
        {
            "src": "/(.*)",
            "dest": "/server/index.js"
        }
    ],
    "buildCommand": "cd ../client && npm run build && cd ../server",
    "installCommand": "npm install && cd ../client && npm install && cd ../server"
}